    <string name="widget_title">Widżet</string>
    <string name="widget_content">Twórz widżety na ekranie głównym. Kliknij je, aby zresetować schowek lub wyświetlić zawartość schowka.\n\nMożesz je znaleźć na liście widżetów.</string>

    <string name="shortcut_title">Skrót</string>
    <string name="shortcut_content">Utwórz skrót do aplikacji na ekranie głównym. Kliknij je, aby zresetować schowek lub wyświetlić zawartość schowka.</string>
    <string name="shortcut_create_clean">Dodaj skrót \"Wyczyść\"</string>
    <string name="shortcut_create_content">Dodaj skrót \"Pokaż zawartość\"</string>
    <string name="shortcut_no_permission">Aplikacja nie ma wystarczających uprawnień</string>
    <string name="shortcut_have_permission">Pozwolenie udzielone! Utwórz teraz skróty.</string>

    <string name="tile_title">Kafelka szybkiego ustawiania</string>
    <string name="tile_content">Dodaj kafelek szybkich ustawień do paska powiadomień. Kliknij je, aby zresetować schowek lub wyświetlić zawartość schowka.\n\nMożesz je znaleźć na ekranie szybkiej edycji ustawień.</string>

    <string name="assistant_title">Aplikacja Asystenta Systemu</string>
    <string name="assistant_content">Ustaw aplikację jako aplikację asystenta systemu i używaj jej, naciskając długo przycisk \"Home\" lub przesuwając palcem z dolnych rogów.</string>
